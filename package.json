		"change-case": "^4.1.2",
		"cosmiconfig": "^9.0.0",
		"cross-spawn": "^7.0.3",
		"debug": "4.1.1",
		"dot": "^1.1.3",
		"find-up": "3.0.0",
		"inflection": "^2.0.1",
		"inquirer": "6.2.2",
		"is": "3.3.0",
		"memfs": "^4.2.1",
		"minimatch": "^9.0.5",
