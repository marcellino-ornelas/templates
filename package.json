{
  "name": "templates",
  "version": "1.0.0",
  "description": "",
  "main": "template.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "build:docs:prod": "NODE_ENV=production npm run build:docs",
    "build:docs:dev": "open -a 'Google Chrome' docs-1/readme.md && nodemon --exec \"npm run build:docs\"",
    "build:docs": "./.bin/generate-docs && ./.bin/generate-docs-doctoc",
    "build:code": "./node_modules/.bin/babel src --out-dir lib",
    "build:dev": "BABEL_ENV=development npm run build:code -- -w",
    "build:prod": "npm run build:code && git add . && git commit -m 'New prod docs' && git push origin master",
    "build": "npm run build:prod",
    "test:run:init": "npm run test:jest -- --runInBand --testPathPattern cli/init -- ",
    "test:core": "npm run build:code && npm run test:jest -- --testPathIgnorePatterns cli/init -- ",
    "test:init": "npm run build:code && echo 'built' && INIT_ONLY=1 ./.bin/run-test",
    "test:jest": "BABEL_ENV=development jest --bail 2",
    "test": "npm run build:code && echo 'built' && ./.bin/run-test",
    "cli": "node ./cli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "doctoc": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.0.0"
  },
  "dependencies": {
    "acorn": "^7.1.0",
    "ansi-colors": "^4.1.1",
    "bluebird": "^3.5.5",
    "debug": "^4.1.1",
    "dot": "^1.1.2",
    "filenamify": "^2.1.0",
    "find-up": "^3.0.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "is": "^3.3.0",
    "prettyjson-256": "^1.5.18",
    "valid-filename": "^2.0.1",
    "yargs": "^13.2.1"
  }
}
