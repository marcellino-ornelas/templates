<table id="create-table">
    <thead>
      <tr>
        <th>Flag</th>
        <th>Default</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
    {{ for(var optionName in create ){ }}{{ var option = create[optionName]; }}<tr>
        <td>--{{= optionName }}, {{= option.alias || '' }}</td>
        <td>{{= option.default || 'N/A' }}</td>
        <td>{{= option.describe || '' }}</td>
      </tr>{{ } }}
    </tbody>
</table>
